<%- include("partials/header") -%>

<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="form-group">
    <label for="#title">Title</label>
    <input class="form-control" id="title" type="text" name="title" placeholder="title">
    <label for="#post">Post</label>
    <textarea class="form-control" id="post" name="post" rows="15" cols="80"></textarea>
  </div>
  <button class="btn btn-md btn-primary" type="submit" name="publish">Publish</button>
</form>

<%- include("partials/footer") -%>
